  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Tests",
      "request": "launch",
      "runtimeArgs": ["run-script", "test:debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "stopOnEntry": true
    },
    {
      "name": "Debug Tests e2e",
      "request": "launch",
      "runtimeArgs": ["run-script", "test:e2e:debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "stopOnEntry": true
    },
    {
      "name": "Debug API",
      "request": "launch",
      "runtimeArgs": ["run-script", "start:debug"],
      "runtimeExecutable": "npm",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    }
  ]
}
